{
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "printWidth": 200,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "importOrder": [
        "^@angular/common(.*)$",
        "^@angular/core(.*)$",
        "^@angular/router$",
        "^@angular/material/(.*)$",
        "^@ngrx/(.*)$",
        "^./auth/(.*)$",
        "^rxjs",
        "/component/(.*)$",
        "/service/(.*)$",
        "/model/(.*)$",
        "<THIRD_PARTY_MODULES>",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderParserPlugins": ["typescript", "jsx", "classProperties", "decorators-legacy"],
    "bracketSameLine": true
}
